[
  {
    "name": "Skipped tests",
    "messageRegex": ".*",
    "matchedStatuses": [ "skipped" ]
  },
  {
    "name": "Element not found",
    "traceRegex": ".*NoSuchElementException thrown.*",
    "matchedStatuses": [ "failed" ]
  },
  {
    "name": "Broken tests",
    "traceRegex": "Error.*",
    "matchedStatuses": [ "failed"]
  },
  {
    "name": "Test defect",
    "matchedStatuses": ["failed"]
  },
  {
    "name": "Product defect",
    "traceRegex": ".*Failed expectation.*",
    "matchedStatuses": [ "failed" ]
  },
  {
    "name": "Passed tests",
    "matchedStatuses": ["passed"]
  }
]